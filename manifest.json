{
  "name": "Dogecoin Twitter/X Logo",
  "description": "Long live the dogecoin",
  "version": "69.420",
  "manifest_version": 3,
  "icons": {
    "48": "/assets/dogecoin.png",
    "128": "/assets/dogecoin.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["/assets/dogecoin.png", "/assets/dogecoin-favicon.png"],
      "matches": ["https://twitter.com/*"]
    }
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://twitter.com/*"
      ]
    }
  ]
}